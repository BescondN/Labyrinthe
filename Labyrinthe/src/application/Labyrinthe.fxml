<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane minHeight="800.0" minWidth="950.0" prefHeight="800.0" prefWidth="950.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LabyrintheController">
   <right>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <VBox layoutX="235.0" prefHeight="800.0" prefWidth="188.0">
               <children>
                  <ComboBox fx:id="Atelier" onAction="#actionAtelier" prefHeight="25.0" prefWidth="163.0" promptText="Selectionner un atelier">
                     <VBox.margin>
                        <Insets bottom="100.0" top="100.0" />
                     </VBox.margin>
                  </ComboBox>
                  <ComboBox fx:id="Algo" onAction="#actionAlgo" prefHeight="25.0" prefWidth="165.0" promptText="Selectionner un algo">
                     <VBox.margin>
                        <Insets bottom="100.0" />
                     </VBox.margin>
                  </ComboBox>
                  <Button fx:id="Lancer" mnemonicParsing="false" onAction="#actionLancer" prefHeight="25.0" prefWidth="164.0" text="Lancer">
                     <VBox.margin>
                        <Insets bottom="100.0" />
                     </VBox.margin>
                  </Button>
                  <Label fx:id="Temps" alignment="CENTER" contentDisplay="RIGHT" prefHeight="31.0" prefWidth="286.0" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS">
                     <font>
                        <Font size="23.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="100.0" />
                     </VBox.margin>
                  </Label>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </right>
   <center>
      <Pane fx:id="pane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
